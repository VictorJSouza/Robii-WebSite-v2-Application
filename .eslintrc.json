{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "Error",
    "no-console": "off",
    "import/extensions": "off",
    "consistent-return": "off",
    "import/prefer-default-export": "off",
    "no-await-in-loop": "off",
    "no-restricted-syntax": "off",
    "no-plusplus": "off",
    "no-restricted-globals": "off",
    "new-cap": [
      "error",
      {
        "properties": false
      }
    ]
  }
}
